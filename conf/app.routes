# microservice specific routes

GET         /new-form/:formTypeId/:version                     @uk.gov.hmrc.gform.controllers.FormController.newForm(formTypeId: FormTypeId, version: Version)
GET         /form/file-upload-page/:fileId                     @uk.gov.hmrc.gform.controllers.FormController.fileUploadPage(fileId: String)

GET         /form                                              @uk.gov.hmrc.gform.controllers.FormController.form()
POST        /form/decision/:formTypeId/:version/:formId        @uk.gov.hmrc.gform.controllers.FormGen.decision(formTypeId: FormTypeId, version: Version, formId: FormId)
GET         /form/:formTypeId/:version/:formId                 @uk.gov.hmrc.gform.controllers.FormGen.formById(formTypeId: FormTypeId, version: Version, formId: FormId)
GET         /form/:formTypeId/:version/:formId/:page           @uk.gov.hmrc.gform.controllers.FormGen.formByIdPage(formTypeId: FormTypeId, version: Version, formId: FormId, page: Int)
POST        /form/:formTypeId/:version                         @uk.gov.hmrc.gform.controllers.FormGen.save(formTypeId: FormTypeId, version: Version, currentPage: Int)

GET         /summary/:formTypeId/:version/:userId/:formId              @uk.gov.hmrc.gform.controllers.SummaryGen.summaryById(formTypeId: FormTypeId, version: Version, formId: FormId, userId: UserId)
POST        /summary/:formTypeId/:version/submit               @uk.gov.hmrc.gform.controllers.SummaryGen.submit(formTypeId: FormTypeId, version: Version)

GET         /assets/*file                                      controllers.Assets.versioned(path="/public", file: Asset)
