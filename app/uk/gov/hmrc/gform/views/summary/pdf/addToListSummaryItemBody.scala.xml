@import play.twirl.api.XmlFormat

@(boldText: String, normalText: XmlFormat.Appendable)
<fo:inline font-size="11px"
           font-family="arial-bold"
           font-weight="bold"
           line-height="11pt">@boldText
    <fo:inline font-size="11px"
               font-family="arial"
               font-weight="normal"
               line-height="11pt">@normalText
    </fo:inline>
</fo:inline>